{
  "name": "week2-sequelize",
  "version": "1.0.0",
  "description": "bootcamp wegodev",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/app.js",
    "migration": "npx sequelize-cli model:generate --models-path ./src/models --migrations-path ./src/migrations --name '%npm_config_table%' --attributes '%npm_config_attributes%'",
    "make:migration": "npx sequelize-cli model:generate --models-path ./src/models --migrations-path ./src/migrations --name $table --attributes $attributes",
    "migrate": "npx sequelize-cli db:migrate --config ./src/config/config.json --migrations-path ./src/migrations",
    "make:seeder": "npx sequelize-cli seed:generate --seeders-path ./src/seeders --name %npm_config_filename%",
    "seeder": "npx sequelize-cli --config ./src/config/config.json --seeders-path ./src/seeders db:seed --seed %npm_config_filename%",
    "seeder:all": "npx sequelize-cli --config ./src/config/config.json --seeders-path ./src/seeders db:seed:all",
    "migrate:rollback": "npx sequelize-cli db:migrate:undo --config ./src/config/config.json --migrations-path ./src/migrations"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmdsfdn/bootcamp-wegodev-nodejs.git"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "mysql"
  ],
  "author": "ahmdsfdn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ahmdsfdn/bootcamp-wegodev-nodejs/issues"
  },
  "homepage": "https://github.com/ahmdsfdn/bootcamp-wegodev-nodejs#readme",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.7",
    "sequelize": "^6.37.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2"
  }  
}
